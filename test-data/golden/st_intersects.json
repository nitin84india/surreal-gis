{
  "description": "PostGIS reference outputs for st_intersects function",
  "reference_version": "PostGIS 3.4",
  "tests": [
    {
      "name": "overlapping_polygons",
      "input": {
        "a": "POLYGON((0 0, 2 0, 2 2, 0 2, 0 0))",
        "b": "POLYGON((1 1, 3 1, 3 3, 1 3, 1 1))"
      },
      "expected": true
    },
    {
      "name": "disjoint_polygons",
      "input": {
        "a": "POLYGON((0 0, 1 0, 1 1, 0 1, 0 0))",
        "b": "POLYGON((5 5, 6 5, 6 6, 5 6, 5 5))"
      },
      "expected": false
    },
    {
      "name": "point_in_polygon",
      "input": {
        "a": "POINT(0.5 0.5)",
        "b": "POLYGON((0 0, 1 0, 1 1, 0 1, 0 0))"
      },
      "expected": true
    },
    {
      "name": "point_outside_polygon",
      "input": {
        "a": "POINT(5 5)",
        "b": "POLYGON((0 0, 1 0, 1 1, 0 1, 0 0))"
      },
      "expected": false
    },
    {
      "name": "touching_polygons",
      "input": {
        "a": "POLYGON((0 0, 1 0, 1 1, 0 1, 0 0))",
        "b": "POLYGON((1 0, 2 0, 2 1, 1 1, 1 0))"
      },
      "expected": true
    },
    {
      "name": "crossing_lines",
      "input": {
        "a": "LINESTRING(0 0, 2 2)",
        "b": "LINESTRING(0 2, 2 0)"
      },
      "expected": true
    }
  ]
}
